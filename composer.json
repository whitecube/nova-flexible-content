{
  "name": "wmt/nova-flexible-content",
  "description": "Flexible Content & Repeater Fields for Laravel Nova.",
  "homepage": "https://github.com/wmt-digital-dev/nova-flexible-content",
  "issues": "https://github.com/wmt-digital-dev/nova-flexible-content",
  "license": "MIT",
  "keywords": [
    "laravel",
    "nova",
    "field",
    "flexible",
    "repeat",
    "group",
    "layout"
  ],
  "authors": [
    {
      "name": "WMT Dev Team",
      "email": "dev@wmt.digital"
    },
    {
      "name": "Toon Van den Bos",
      "email": "toon@whitecube.be"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "laravel/framework": "^11.0",
    "laravel/nova": "^4.0",
    "nova-kit/nova-packages-tool": "^1.3.1"
  },
  "autoload": {
    "psr-4": {
      "Wmt\\NovaFlexibleContent\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Wmt\\NovaFlexibleContent\\FieldServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "laravel/pint": "^1.17",
    "psalm/plugin-laravel": "^2.0",
    "orchestra/testbench": "^9.2"
  },
  "autoload-dev": {
    "psr-4": {
      "Wmt\\NovaFlexibleContent\\Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "scripts": {
    "test": "./vendor/bin/phpunit --colors=always --configuration=phpunit.xml.dist tests",
    "fix-style": "./vendor/bin/pint"
  }
}
